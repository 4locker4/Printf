program:
 make asm
 make main
 make all

asm:
 nasm My_Printf.asm -g -f elf64 -l My_Printf.txt -o MyPrintf.o

main:
 gcc -g -I./include -c src/main.c -o build/main.o

all:
 gcc -g -no-pie -Wl,-z,noexecstack build/main.o build/MyPrintf.o -o build/program

run:
 ./build/program

debug:
 gdb ./build/program